{
  "name": "verb-generate-readme",
  "description": "Generate your project's readme with verb. Requires verb v0.9.0 or higher.",
  "version": "0.1.28",
  "homepage": "https://github.com/verbose/verb-generate-readme",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "verbose/verb-generate-readme",
  "bugs": {
    "url": "https://github.com/verbose/verb-generate-readme/issues"
  },
  "license": "MIT",
  "files": [
    "generator.js",
    "index.js",
    "lib",
    "LICENSE",
    "README.md",
    "templates"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=4.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "arr-diff": "^3.0.0",
    "assemble-handle": "^0.1.2",
    "assemble-loader": "^0.6.1",
    "async-each": "^1.0.0",
    "base-fs-conflicts": "^0.1.7",
    "base-pkg": "^0.2.4",
    "base-questions": "^0.7.1",
    "camel-case": "^3.0.0",
    "debug": "^2.2.0",
    "delete": "^0.3.2",
    "engine-handlebars": "^0.8.0",
    "fs-exists-sync": "^0.1.0",
    "get-value": "^2.0.6",
    "gulp-reflinks": "^0.2.0",
    "has-value": "^0.3.1",
    "is-valid-app": "^0.2.0",
    "lazy-cache": "^2.0.1",
    "log-utils": "^0.2.1",
    "markdown-toc": "^0.12.14",
    "match-file": "^0.2.0",
    "mixin-deep": "^1.1.3",
    "resolve": "^1.1.7",
    "sections": "^0.1.8",
    "template-helpers": "^0.6.6",
    "update-sections": "^0.1.0",
    "verb-collections": "^0.1.4",
    "verb-defaults": "^0.2.0",
    "verb-repo-data": "^0.1.11",
    "verb-repo-helpers": "^0.2.3",
    "verb-toc": "^0.2.8"
  },
  "devDependencies": {
    "generate": "^0.9.8",
    "global-modules": "^0.2.2",
    "gulp": "^3.9.1",
    "gulp-eslint": "^3.0.1",
    "gulp-format-md": "^0.1.9",
    "gulp-istanbul": "^1.0.0",
    "gulp-mocha": "^2.2.0",
    "gulp-unused": "^0.1.2",
    "mocha": "^2.5.3"
  },
  "keywords": [
    "docs",
    "documentation",
    "generate",
    "generator",
    "markdown",
    "plugin",
    "readme",
    "template",
    "templates",
    "verb",
    "verb-generator",
    "verbgenerator",
    "verbplugin"
  ],
  "verb": {
    "toc": {
      "render": true,
      "method": "preWrite",
      "maxdepth": 4
    },
    "layout": "common-minimal",
    "tasks": [
      "readme"
    ],
    "middleware": {
      "prePipeline": [
        {
          "name": "verb-reflinks",
          "match": "*.md"
        }
      ]
    },
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "highlight": "generate",
      "list": [
        "assemble",
        "generate",
        "update",
        "verb",
        "verb-collections",
        "verb-repo-data",
        "verb-toc"
      ]
    },
    "reflinks": [
      "gulp",
      "verb-generate-readme"
    ],
    "sections": false,
    "lint": {
      "reflinks": true
    }
  }
}
