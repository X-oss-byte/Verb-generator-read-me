{
  "name": "verb-readme-generator",
  "description": "Generate your project's readme with verb. Requires verb v0.9.0 or higher.",
  "version": "0.1.2",
  "homepage": "https://github.com/verbose/verb-readme-generator",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "verbose/verb-readme-generator",
  "bugs": {
    "url": "https://github.com/verbose/verb-readme-generator/issues"
  },
  "license": "MIT",
  "files": [
    "lib/",
    "templates/",
    "index.js",
    "generator.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "async": "^1.5.2",
    "debug": "^2.2.0",
    "extend-shallow": "^2.0.1",
    "generate-defaults": "^0.1.5",
    "get-pkg": "^0.2.0",
    "get-value": "^2.0.3",
    "git-user-name": "^1.1.2",
    "helper-apidocs": "^0.5.1",
    "helper-copyright": "^2.0.1",
    "helper-date": "^0.2.2",
    "helper-issue": "^0.3.0",
    "helper-reflinks": "^3.1.0",
    "helper-related": "^0.13.0",
    "is-affirmative": "^0.1.0",
    "lazy-cache": "^1.0.3",
    "omit-empty": "^0.3.2",
    "parse-author": "^0.2.0",
    "update-sections": "^0.1.0",
    "verb-toc": "^0.1.0"
  },
  "devDependencies": {
    "gulp-format-md": "*",
    "mocha": "*"
  },
  "keywords": [
    "generate",
    "generator",
    "markdown",
    "readme",
    "template",
    "templates",
    "verb",
    "verb-generator",
    "verbgenerator"
  ],
  "verb": {
    "toc": false,
    "layout": "default-dev",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "highlight": "generate",
      "list": [
        "assemble",
        "generate",
        "verb"
      ]
    },
    "reflinks": [
      "verb"
    ]
  }
}
